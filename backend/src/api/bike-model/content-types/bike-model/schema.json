{
  "kind": "collectionType",
  "collectionName": "bike_models",
  "info": {
    "singularName": "bike-model",
    "pluralName": "bike-models",
    "displayName": "BikeModel",
    "description": "Modelos de Motos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "bike_models"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "motos_compatibles"
    }
  }
}
